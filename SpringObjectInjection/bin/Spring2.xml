<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
     http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
     http://www.springframework.org/schema/context
     http://www.springframework.org/schema/context/spring-context-4.0.xsd">
	
		 




<bean id="mob" class="com.cg.SpringObjectinjection.service.Mobile">
<property name="mobileid" value="132"></property>
<property name="mobileName" value="Apple 7"></property>
<property name="mobileprice" value="40000"></property>
<property name="invent" ref="inv"></property>
</bean>	 

<bean id="inv"  class="com.cg.SpringObjectinjection.service.Inventory">
<property name="mobilecount" value="100"></property>
<property name="mobilecompany" value="Apple"></property>

</bean>
</beans>